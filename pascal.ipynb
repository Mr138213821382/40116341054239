      "source": [
        "def pascal(n):\n",
        "  if n == 1:\n",
        "    return [1]\n",
        "  else:\n",
        "    x = [1]\n",
        "    p = pascal(n-1)\n",
        "    for i in range(len(p) - 1):\n",
        "      x.append(p[i] + p[i+1])\n",
        "    x += [1]\n",
        "  return x  "
      ],
